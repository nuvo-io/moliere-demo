#!/bin/bash

if [ $# -lt 2 ]; then
    echo "USAGE:"
    echo "        ishapes  <deviceID> <pleth-period>"
    exit
fi

DID=$1
PPERIOD=$2

VERSION=-0.11.0-SNAPSHOT
JAR=`dirname $0`/../target/scala-2.11/moliere-demo-assembly${VERSION}.jar

java \
    -Dddsi.network.interface=en1 \
    -cp ${JAR} \
    dds.demo.oximeter.Oximeter ${DID} ${PPERIOD}
    

