#!/bin/bash

VERSION=-0.11.0-SNAPSHOT
JAR=`dirname $0`/../target/scala-2.11/moliere-demo-assembly${VERSION}.jar


if [ $# -lt 3 ]; then
    echo "USAGE:"
    echo "        SHeatMap <cooling-period> <in-partition> <out-partition>"
    exit
fi


java \
    -Dddsi.network.interface=en0 \
    -cp ${JAR} \
     dds.demo.shapes.HeatMap \
     48 64 640 480 $1 $2 $3
     

